<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/main_navgraph"
	app:startDestination="@id/tilesFragment">

	<fragment
		android:id="@+id/tilesFragment"
		android:name="ru.binaryunicorn.coloria.presentation.tiles.views.TilesFragment"
		android:label="TilesFragment"
		tools:layout="@layout/fragment_tiles">
		<action
			android:id="@+id/action_tilesFragment_to_settingsFragment"
			app:destination="@id/settingsFragment" />
	</fragment>
	<fragment
		android:id="@+id/settingsFragment"
		android:name="ru.binaryunicorn.coloria.presentation.settings.views.SettingsFragment"
		android:label="SettingsFragment"
		tools:layout="@layout/fragment_settings" />
</navigation>
